{
  "name": "@nojaja/greputil",
  "version": "1.0.5",
  "description": "複数の正規表現パターンを効率的に扱うためのJavaScriptユーティリティライブラリ。文字列の一括パターンマッチングとバッファパターンマッチング機能を提供します。",
  "main": "./dist/GrepUtil.bundle.js",
  "keywords": [
    "grep",
    "regex",
    "regexp",
    "pattern-matching",
    "buffer",
    "search",
    "text-processing",
    "utility",
    "multiple-patterns",
    "string-matching"
  ],
  "scripts": {
    "test": "jest --config jest.unit.config.js",
    "test:ci": "jest --config jest.unit.config.js --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:all": "jest --config jest.unit.config.js ; jest --config jest.integration.config.js",
    "build": "webpack && tsc --emitDeclarationOnly",
    "lint": "eslint .",
    "depcruise": "depcruise --config .dependency-cruiser.js src",
    "docs": "typedoc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nojaja/NodeGrepUtil.git"
  },
  "author": "nojaja <free.riccia@gmail.com> (https://github.com/nojaja)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nojaja/NodeGrepUtil/issues"
  },
  "homepage": "https://github.com/nojaja/NodeGrepUtil#readme",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "cross-env": "^7.0.3",
    "dependency-cruiser": "^17.3.5",
    "eslint": "^8.45.0",
    "eslint-plugin-jsdoc": "^46.0.0",
    "eslint-plugin-sonarjs": "^0.19.0",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.4",
    "typescript": "^5.3.3",
    "typedoc": "^0.25.0",
    "typedoc-plugin-markdown": "^3.16.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
